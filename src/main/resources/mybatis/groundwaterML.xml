<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- Common blocks for WaterML queries -->
<mapper namespace="groundwaterML">
	<resultMap id="pointResult" type="WaterMLPoint">
		<result property="featureId" column="featureId"/>
		<result property="monLocReference" column="monLocReference"/>
		<result property="siteName" column="siteName"/>
		<result property="interpolationType" column="interpolationType"/>
		<result property="phenomenonTimeStart" column="phenomenonTimeStart"/>
		<result property="phenomenonTimeEnd" column="phenomenonTimeEnd"/>
		<result property="resultDateTimeUTC" column="resultDateTimeUTC"/>
		<result property="resultValue" column="resultValue"/>
		<result property="resultUnit" column="resultUnit"/>
		<result property="pcode" column="pcode"/>
		<result property="pcodeDesc" column="pcodeDesc"/>
		<result property="pcodeReference" column="pcodeReference"/>
		<result property="statisticDesc" column="statisticDesc"/>
		<result property="statisticReference" column="statisticReference"/>
		<result property="approvalsAsJson" column="approvalsAsJson"/>
		<result property="qualifiersAsJson" column="qualifiersAsJson"/>
		<result property="status" column="status"/>
	</resultMap>

</mapper>